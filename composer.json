{
  "name": "mondago/application-insights",
  "description": "This project extends the Application Insights API surface to support PHP.",
  "type": "library",
  "keywords": [
    "log",
    "logging",
    "telemetry",
    "insights",
    "monitoring"
  ],
  "homepage": "https://github.com/mondago/ApplicationInsights-PHP",
  "license": "MIT",
  "require": {
    "php": "^7.3|^8.0",
    "guzzlehttp/guzzle": "^7.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "evert/phpdoc-md": "~0.0.7"
  },
  "autoload": {
    "psr-4": {
      "ApplicationInsights\\": "ApplicationInsights/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ApplicationInsights\\Tests\\": "Tests/"
    }
  }
}
